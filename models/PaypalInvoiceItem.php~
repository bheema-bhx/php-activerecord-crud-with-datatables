<?php
class PaypalInvoiceItem extends ActiveRecord\Model
{

public static $table_name="paypal_invoice_item";
public static $primary_key = 'customer_id';

static $belongs_to = array(
        array(
            'brokers',
            'foreign_key' => 'customer_id',
            'primary_key' => 'broker_id',
            'class_name' => 'Broker'
        )
    );   
 
    
}
?>
